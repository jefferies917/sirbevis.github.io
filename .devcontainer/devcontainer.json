{
    "name": "Jekyll Dev Container",
    "image": "mcr.microsoft.com/devcontainers/ruby:3.2",
    "remoteUser": "vscode",
    "forwardPorts": [
        4000
    ],
    "postCreateCommand": "gem install bundler && bundle install",
    "postStartCommand": "bundle exec jekyll serve --livereload --host 0.0.0.0 --baseurl ''"
}